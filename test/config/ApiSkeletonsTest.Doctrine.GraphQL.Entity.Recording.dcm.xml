<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ApiSkeletonsTest\Doctrine\GraphQL\Entity\Recording">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="source" type="text" nullable="false"/>
    <many-to-one field="performance" target-entity="ApiSkeletonsTest\Doctrine\GraphQL\Entity\Performance" inversed-by="recordings">
      <join-columns>
        <join-column name="performance_id" referenced-column-name="id" nullable="false"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="users" target-entity="ApiSkeletonsTest\Doctrine\GraphQL\Entity\User" mapped-by="recordings"/>
  </entity>
</doctrine-mapping>
