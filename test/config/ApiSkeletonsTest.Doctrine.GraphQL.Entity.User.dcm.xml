<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ApiSkeletonsTest\Doctrine\GraphQL\Entity\User">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="name" type="string" nullable="false"/>
    <field name="email" type="string" nullable="false"/>
    <many-to-many field="recordings" target-entity="ApiSkeletonsTest\Doctrine\GraphQL\Entity\Recording" inversed-by="users">
      <join-table name="RecordingToUser">
        <join-columns>
          <join-column name="user_id" referenced-column-name="id" nullable="false"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="recording_id" referenced-column-name="id" nullable="false"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
