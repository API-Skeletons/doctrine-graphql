<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ApiSkeletonsTest\Doctrine\GraphQL\Entity\Performance">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="venue" type="string" nullable="true"/>
    <field name="city" type="string" nullable="true"/>
    <field name="state" type="string" nullable="true"/>
    <field name="performanceDate" type="datetime" nullable="false"/>
    <many-to-one field="artist" target-entity="ApiSkeletonsTest\Doctrine\GraphQL\Entity\Artist" inversed-by="performances">
      <join-columns>
        <join-column name="artist_id" referenced-column-name="id" nullable="false"/>
      </join-columns>
    </many-to-one>
    <one-to-many field="recordings" target-entity="ApiSkeletonsTest\Doctrine\GraphQL\Entity\Recording" mapped-by="performance"/>
  </entity>
</doctrine-mapping>
