<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="DbTest\Entity\User">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="name" nullable="true"/>
    <one-to-one field="address" target-entity="DbTest\Entity\Address" mapped-by="user"/>
    <many-to-many field="artist" target-entity="DbTest\Entity\Artist" mapped-by="user"/>
  </entity>
</doctrine-mapping>
