{
    "name": "api-skeletons/doctrine-graphql",
    "description": "Framework Agnostic GraphQL for Doctrine using Attributes",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom H Anderson",
            "email": "tom.h.anderson@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "doctrine/orm": "^2.8",
        "doctrine/doctrine-laminas-hydrator": "^2.2",
        "api-skeletons/doctrine-querybuilder-filter": "^1.2",
        "webonyx/graphql-php": "^v14.0.2"
    },
    "autoload": {
        "psr-4": {
            "ApiSkeletons\\Doctrine\\GraphQL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApiSkeletonsTest\\Doctrine\\GraphQL\\": "test/"
        }
    },
    "require-dev": {
        "doctrine/dbal": "^3.1.1",
        "symfony/cache": "^5.3",
        "doctrine/coding-standard": "^9.0",
        "doctrine/dbal": "^2.13.7||^3.1.1",
        "doctrine/annotations": "^1.13.2",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.21"
    },
    "scripts": {
        "test": "vendor/bin/phpcs && vendor/bin/phpunit && vendor/bin/psalm"
    }
}
